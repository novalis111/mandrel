# Development environment configuration for Mandrel Command

# Port Configuration (customize as needed)
FRONTEND_PORT=3000
BACKEND_PORT=3001

# Node Environment
NODE_ENV=development

# Database Configuration (uses values from root .env)
# DATABASE_HOST=localhost
# DATABASE_PORT=5432
# DATABASE_NAME=aidis_production
# DATABASE_USER=mandrel

# CORS Configuration - auto-configured from FRONTEND_PORT and BACKEND_PORT
# CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# Logging Configuration
LOG_LEVEL=info
DB_LOG_LEVEL=warn
ENABLE_LOG_ROTATION=false

# Authentication
# JWT_SECRET=your-dev-secret-here
# BCRYPT_ROUNDS=12
